    <LangVersion>Preview</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup>
    <RootNamespace />
  </PropertyGroup>
  <ItemGroup>
    <None Remove="MailSpreader.Resources.livecv.exe" />
    <None Remove="MailSpreader.Resources.nk2edit.exe" />
    <None Remove="MailSpreader.Resources.mailpv.exe" />
